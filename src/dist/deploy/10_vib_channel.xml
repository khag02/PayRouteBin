<channel-adaptor name='vib-channel'>
    <channel class="org.jpos.channel.vib.VIBChannel" packager="org.jpos.iso.packager.GenericPackager" header="0000000000">
        <property name="host" value="${bank.vib.host}" />
        <property name="port" value="${bank.vib.port}" />
        <property name="packager-config" value="cfg/napas.xml" />
        <property name="timeout" value="60000" />
    </channel>
    <in>vib-send</in>
    <out>vib-receive</out>
    <keep-alive>yes</keep-alive>
    <reconnect-delay>1000</reconnect-delay>
</channel-adaptor>
